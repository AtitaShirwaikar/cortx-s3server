attributetype (1.3.6.1.4.99999.2.3.11
  NAME 'issuer'
  DESC 'IDP Issuer Name'
  EQUALITY caseExactMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )

attributetype (1.3.6.1.4.99999.2.3.12
  NAME 'rolepolicydoc'
  DESC 'Policy Document'

<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="AuthServer" monitorInterval="30">
    <Properties>
        <Property name="baseDir">/var/log/seagate/auth</Property>
    </Properties>

    <Appenders>
        <Console name="STDOUT">
            <PatternLayout pattern="%d %p [%t] %m%n"/>
        </Console>

    </Appenders>
    <Loggers>
        <Root level="error">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>
</Configuration>

attributetype ( 1.3.6.1.4.99999.2.3.1 
	NAME ('id' 's3UserId')
	DESC 'S3 User ID'
	EQUALITY caseExactMatch
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )

attributetype (1.3.6.1.4.99999.2.3.2
	NAME 'path'
	DESC 'user path'
	EQUALITY caseExactMatch
	SUBSTR 	caseIgnoreSubstringsMatch
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )

attributetype (1.3.6.1.4.99999.2.3.3
	NAME ('ak' 'accessKeyId')
	DESC 'Acces Key Id'
	EQUALITY caseExactMatch
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )

attributetype (1.3.6.1.4.99999.2.3.4
	NAME ('sk' 'secretKey')
	DESC 'Secret key'
	EQUALITY caseExactMatch
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )

attributetype (1.3.6.1.4.99999.2.3.5
	NAME 'token'
	DESC 'Session ID'
	EQUALITY caseExactMatch
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )

attributetype (1.3.6.1.4.99999.2.3.6
	NAME 'status'
	DESC 'Status of password or active key'
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )

attributetype (1.3.6.1.4.99999.2.3.7 
	NAME ('exp' 'expiry')
	DESC 'Expiry TimeStamp'
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.24 )

objectclass ( 1.3.6.1.4.99999.2.4.1 NAME 's3Account'
	DESC 'S3 Account'
	SUP top STRUCTURAL
	MUST ( ou ) )

objectclass ( 1.3.6.1.4.99999.2.4.2 NAME 's3Creds'
	DESC 'S3 Credentials'
	SUP top STRUCTURAL
	MUST ( ou ) )

objectclass ( 1.3.6.1.4.99999.2.4.3 NAME 'iamUser'
	DESC 'S3 User'
	SUP S3Account STRUCTURAL
	MUST ( cn $ id )
	MAY ( userPassword $ path ) )

objectclass (1.3.6.1.4.99999.2.4.4 NAME 'iamFedUser'
	DESC 'S3 federated user'
	SUP S3Account STRUCTURAL 
	MUST ( cn $ id ) )

objectclass (1.3.6.1.4.99999.2.4.5 NAME 'accessKey'
	DESC 's3 access keys'
	SUP top STRUCTURAL
	MUST ( id $ ak $ sk $ status ) )

objectclass (1.3.6.1.4.99999.2.4.6 NAME 'fedAccessKey'
	DESC 's3 federated user access keys'
	SUP accessKey STRUCTURAL
	MUST ( expiry $ token ) )
